{
  "name": "vue-displacement-slideshow",
  "version": "4.0.0",
  "description": "A Displacement slideshow VueJs component made with Three.js and GSAP",
  "author": "AlbanCrepel <alban.crepel@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/lib/vue-displacement-slideshow.umd.js",
  "module": "./dist/lib/vue-displacement-slideshow.es.js",
  "exports": {
    ".": {
      "import": "./dist/lib/vue-displacement-slideshow.es.js",
      "require": "./dist/lib/vue-displacement-slideshow.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --mode=lib",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlbanCrepel/vue-displacement-slideshow.git"
  },
  "dependencies": {
    "gsap": "^3.8.0",
    "three": "^0.134.0",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "vite": "^2.6.14"
  },
  "keywords": [
    "vue",
    "three",
    "webgl",
    "gsap",
    "slideshow"
  ]
}
